{
	"presets": [
		["@babel/env", {
			"modules": false,
			"targets": {
				"node": "current"
			}
		}]
	],
	"plugins": [
		["@babel/plugin-proposal-decorators", { "legacy": true }],
		["@babel/plugin-proposal-class-properties", { "loose": true }],
		["@babel/plugin-proposal-object-rest-spread", { "useBuiltIns": true }],
		["@babel/plugin-proposal-do-expressions"]
	]
}
