{
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true
	},
	"extends": "prettier",
	"parserOptions": {
		"sourceType": "module"
	},
	"rules": {
		"indent": 0,
		"quotes": 0,
		"linebreak-style": ["error", "unix"],
		"semi": ["error", "always"],
		"valid-jsdoc": ["warn", {
			"requireReturn": false,
			"requireParamDescription": false,
			"requireReturnDescription": false
		}]
	}
}
